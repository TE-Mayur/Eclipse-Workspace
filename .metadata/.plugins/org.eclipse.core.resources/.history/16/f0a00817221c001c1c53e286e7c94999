package com.company.yantra;

public class Duplicate {
	public static int samenumber(int array[],int n) {
		if(n == 0 || n == 1) {
			return n;
		}
	int[] temp= new int[n];
	int j=0;
	
	for(int i = 0; i< n-1; i++) {
		if(array[i] != array[i+1]) {
			temp[j++]=array[i];
		}
	}
	temp[j++]=array[n-1];
	
	for (int i = 0;i<j;i++) {
		array[i]=temp[i];
		}
	return j;
	}
	public static void main(String[] args) {
		int array[]= {1,5,6,5,1,2,3,4,4,6};
		int n = array.length;
		n = samenumber(array,n);
		
		for(int i=0;i<n;i++)
		System.out.print(array[i]+"");	
		}
	}

